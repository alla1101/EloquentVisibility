{
    "name": "alla1101/eloquentvisibility",
    "description": "Package to Add visibility feature to each row of each model you choose",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alla Barkat",
            "email": "a2la1101@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Alla\\Visibility\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alla\\Visibility\\EloquentVisibilityServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "7.x-dev",
        "illuminate/console": "7.x-dev",
        "illuminate/filesystem": "7.x-dev",
        "illuminate/database": "7.x-dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Alla\\Visibility\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "orchestra/testbench": "^5.0"
    }
}
